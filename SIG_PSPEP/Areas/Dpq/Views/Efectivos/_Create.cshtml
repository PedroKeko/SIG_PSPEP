                        <select asp-for="OrgaoUnidadeId" class="select2 form-select form-control-sm input-1 small-input" asp-items="ViewBag.OrgaoUnidadeId" style="width: 100%;"></select>
                    </div>
                </div>
                <div class="col-6 col-md-4">
                    <div class="form-group">
                        <label asp-for="FuncaoCargoId" class="control-label small-label"></label>
                        <select asp-for="FuncaoCargoId" class="select2 form-select form-control-sm input-1 small-input" asp-items="ViewBag.FuncaoCargoId" style="width: 100%;"></select>
