<script>
    $(document).ready(function () {
        // Quando uma província for selecionada, carrega os municípios
        $("#ProvinciaResidenciaId").change(function () {
            var provinciaId = $(this).val();

            // Limpa as opções de município e comuna
            $("#MunicipioResidenciaId").empty().append('<option value="">-- Selecione Município --</option>');

            if (provinciaId) {
                $.ajax({
                    url: '@Url.Action("GetMunicipios", "Efectivos")', // Ajuste o nome do controlador se necessário
                    data: { provinciaId: provinciaId },
                    success: function (municipios) {
                        // Adiciona os municípios ao select
                        $.each(municipios, function (index, municipio) {
                            $("#MunicipioResidenciaId").append('<option value="' + municipio.id + '">' + municipio.nome + '</option>');
                        });
                    },
                    error: function (xhr, status, error) {
                        console.error("Erro ao carregar municípios: " + error);
                    }
                });
            }
        });
    });
</script>

