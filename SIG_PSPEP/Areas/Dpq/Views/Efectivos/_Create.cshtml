                            <label asp-for="ProvinciaResId" class="control-label small-label"></label>
                            <select asp-for="ProvinciaResId" class="select2 form-select form-control-sm input-1 small-input" id="ProvinciaResidenciaId" asp-items="ViewBag.ProvinciaResId" style="width: 100%;" required>
