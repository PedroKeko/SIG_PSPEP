                <div class="col-6 col-md-4">
                    <div class="form-group">
                        <div class="form-group">
                            <label asp-for="MunicipioId" class="control-label small-label"></label>
                            <select asp-for="MunicipioId" class="select2 form-select form-control-sm input-1 small-input" id="MunicipioResidenciaId" asp-items="ViewBag.MunicipioId" style="width: 100%;" required>
