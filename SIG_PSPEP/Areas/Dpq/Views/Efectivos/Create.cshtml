        .formbold-form-step-2.active,
        .formbold-form-step-3.active {
            display: block;
        }

    .formbold-form-btn-wrapper {
        display: flex;
        align-items: center;
        justify-content: flex-end;
        gap: 25px;
        margin-top: 25px;
    }

