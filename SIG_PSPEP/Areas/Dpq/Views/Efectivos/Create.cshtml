                    <label class="control-label small-label">Tipo de Documento</label>
                    <select class="form-select form-control-sm input-1 small-input">
                        <option value="BI">Bilhete de Identidade</option>
                        <option value="Passaporte">Passaporte</option>
                        <option value="Outros">Outros</option>
                    </select>
                </div>
            </div>
            <div class="col-6 col-md-3">
                <!-- Número de Documento -->
                <div class="form-group">
                    <label asp-for="NumBI" class="control-label small-label"></label>
                    <input asp-for="NumBI" class="form-control form-control-sm input-1 small-input" placeholder="Nº de Documento" required />
                    <span asp-validation-for="NumBI" class="text-danger"></span>
                </div>
