                        <option value="Viuvo">Viuvo</option>
                    </select>
                    <span asp-validation-for="EstadoCivil" class="text-danger"></span>
                </div>
            </div>
            <div class="col-6 col-md-3">
                <!-- Tipo de Documento -->
