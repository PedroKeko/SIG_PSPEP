        display: flex;
        gap: 20px;
        margin-bottom: 10px;
    }

        .formbold-input-flex > div {
            width: 50%;
        }

    .formbold-form-input {
        width: 100%;
        padding: 8px 14px;
        border-radius: 5px;
        border: 1px solid #DDE3EC;
